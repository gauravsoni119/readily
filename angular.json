    },
    "dashboard-ui": {
      "projectType": "library",
      "root": "libs/dashboard/ui",
      "sourceRoot": "libs/dashboard/ui/src",
      "prefix": "readily",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/dashboard/ui"],
