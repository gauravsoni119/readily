          "outputs": ["coverage/libs/shell/feature"],
          "options": {
            "jestConfig": "libs/shell/feature/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shell/feature/src/**/*.ts",
              "libs/shell/feature/src/**/*.html"
            ]
          }
        }
      }
    },
    "shell-layout": {
      "projectType": "library",
      "root": "libs/shell/layout",
      "sourceRoot": "libs/shell/layout/src",
      "prefix": "readily",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shell/layout"],
          "options": {
            "jestConfig": "libs/shell/layout/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shell/layout/src/**/*.ts",
              "libs/shell/layout/src/**/*.html"
            ]
          }
        }
      }
    },
    "shared-ui": {
      "projectType": "library",
      "root": "libs/shared/ui",
      "sourceRoot": "libs/shared/ui/src",
      "prefix": "readily",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/ui"],
          "options": {
            "jestConfig": "libs/shared/ui/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/ui/src/**/*.ts",
              "libs/shared/ui/src/**/*.html"
            ]
          }
        }
      }
    },
    "dashboard-feature": {
      "projectType": "library",
      "root": "libs/dashboard/feature",
      "sourceRoot": "libs/dashboard/feature/src",
      "prefix": "readily",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/dashboard/feature"],
